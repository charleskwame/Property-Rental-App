{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node",
            "config": {
                "zeroConfig": true
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "MONGODB_URI": "@MONGODB_URI",
        "JWT_SECRET": "@JWT_SECRET",
        "JWT_EXPIRES_IN": "@JWT_EXPIRES_IN",
        "NODE_ENV": "@NODE_ENV"
    }
}